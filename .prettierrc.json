{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "jsxSingleQuote": false,
  "bracketSameLine": true,
  "singleAttributePerLine": false,
  "endOfLine": "lf"
}
